<!-- Задание 1.1: Логические операции -->
<h3>1.1 Логические операции для чисел 7 и 3</h3>
<?php
$a = 7;
$b = 3;

echo "AND: $a & $b = " . ($a & $b) . "<br>";
echo "OR:  $a | $b = " . ($a | $b) . "<br>";
echo "XOR: $a ^ $b = " . ($a ^ $b) . "<br>";
echo "NOT A: ~$a = " . ~$a . "<br>";
echo "NOT B: ~$b = " . ~$b;
?>
